<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite configfailurepolicy="continue" name="Test Suite">
    <listeners>
        <listener class-name="auto.listeners.TestListener"/>
    </listeners>

    <parameter name="platform" value="chrome"/>

    <test name="LOGIN">
        <classes>
            <class name="auto.login.LoginPositiveTest"/>
            <class name="auto.login.LoginNegativeTest"/>
        </classes>
    </test>

</suite>
